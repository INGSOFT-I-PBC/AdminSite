{
    "files": [],
    "references": [
        {
            "path": "./tsconfig.config.json"
        },
        {
            "path": "./tsconfig.app.json"
        },
        {
            "path": "./tsconfig.vitest.json"
        }
    ],
    "compilerOptions": {
        "isolatedModules": true,
        "types": ["types/auto-imports.d.ts", "src/components.d.ts"],
        "strict": false,
        "allowJs": true,
        "pretty": true,
        "moduleResolution": "Node",
        "paths": {
            "@directives": ["./src/directives"],
            "@store": ["./src/store"],
            "@store-types": ["./src/store/types"],
            "@types": ["./src/types"],
            "@views/*": ["./src/views/*"],
            "@store/*": ["./src/store/*"],
            "@exceptions": ["./src/exceptions"]
        },
        "noImplicitAny": false,
        "noImplicitThis": true
    }
}
